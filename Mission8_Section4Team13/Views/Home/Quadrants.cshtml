@{
    ViewData["Title"] = "Quadrants";
}
@model List<TaskResponse>

<table class="table table-bordered table-striped">

    <thead class="font-weight-bold">
        <tr>
            <td>Task</td>
            <td>Due Date</td>
            <td>Quadrant</td>
            <td>Complete</td>
            <td>Category</td>
        </tr>
    </thead>

    <tbody>
        @foreach (var x in Model) //model does need to be "Model"
        {
        <tr>
            <td>@x.task</td>
            <td>@x.dueDate</td>
            <td>@x.quadrant</td>
            <td>@x.completed</td>
            <td>@x.category.categoryName</td>
            <!--
    <td><a asp-controller="Home" asp-action="Edit" asp-route-FilmID="@x.taskId">Edit</a></td>
    <td><a asp-controller="Home" asp-action="delete" asp-route-FilmID="@x.taskId">Delete</a></td>-->

        </tr>
        }
        <tr></tr>
    </tbody>

</table>
